<Page x:Class="MahAppsMetroMvvmLight.View.PageOne"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MahAppsMetroMvvmLight.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageOne"
      DataContext="{Binding PageOneViewModel,Source={StaticResource Locator}}">

    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="325,200,0,0" TextWrapping="Wrap" TextAlignment="Center" FontSize="20" Text="{Binding PageOneText}" VerticalAlignment="Top" Height="50" Width="150"/>
    </Grid>
</Page>
